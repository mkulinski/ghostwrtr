<div class="lyric col-xs-10 col-xs-offset-1 col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3">
    <h1>admin/Users index page</h1>

    <% @users.each do |user| %>
        <div class="card">
            <%= link_to 'Edit User Registration', edit_user_registration_path(user), class: 'pull-right' %>
          <%= link_to user_path(user) do %>
          <%= image_tag user.image, class: 'img-responsive' %>
          <br />
          <div class="handle"><%= user.username %></div>
          <div class="short-line">"<%= user.email %>"</div>
          <div class="long-handle">Sign in count = <%= user.sign_in_count %></div>
          <div class="long-handle">User created at = <%= user.created_at %></div>
          <div class="long-handle">User last sign in at = <%= user.last_sign_in_at %></div>
          <% end %>
          <br class="clr" />
        </div>
    <% end %>
</div>